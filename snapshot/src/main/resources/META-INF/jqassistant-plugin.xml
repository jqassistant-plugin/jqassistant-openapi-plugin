        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.IntegerTypeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.JsonSchemaDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.NullTypeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.NumberTypeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.ObjectTypeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.TypeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.ReferenceTypeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.SchemaDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.StringTypeDescriptor</class>
    </model>
