        <class>org.jqassistant.plugin.openapi.api.OpenApiScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.plugin.openapi.impl.OpenAPIScannerPlugin</class>
    </scanner>
</jqassistant-plugin>