<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.10"
                    name="jQAssistant OpenApi Plugin">
    <description>Ziel ist die Erstellung eines jQAssistant Plugins zur Validierung von OpenAPI Files.</description>
    <model>
        <class>org.jqassistant.plugin.openapi.api.model.ContactDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ContractDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.MediaTypeObjectDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.OperationDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ParameterDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.PathItemDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ComponentsDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.SecuritySchemeDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.CallbackDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ExampleDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.HeaderDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.LinkDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.RequestBodyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ResponseDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ServerDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.TagDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.PathsDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ExternalDocsDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ServerVariableDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.ArrayPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.BoolPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.DiscriminatorDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.EnumStringPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.EnumValueDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.IntegerPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.JsonSchemaDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.NullPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.NumberPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.ObjectPropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.PropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.ReferencePropertyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.SchemaDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.jsonschema.StringPropertyDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.openapi.impl.OpenAPIScannerPlugin</class>
    </scanner>
</jqassistant-plugin>