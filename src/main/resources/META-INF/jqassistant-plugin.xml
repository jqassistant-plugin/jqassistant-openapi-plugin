<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.10"
                    name="jQAssistant OpenApi Plugin">
    <description>Ziel ist die Erstellung eines jQAssistant Plugins zur Validierung von OpenAPI Files.</description>
    <model>
        <class>org.jqassistant.plugin.openapi.api.model.ContactDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ContractDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.MediaTypeObjectDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.OperationDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ParameterDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.PathDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ComponentsDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.SecuritySchemaDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.CallbackDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ExampleDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.SchemaDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.HeaderDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.LinkDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.RequestBodyDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ResponseDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.ServerDescriptor</class>
        <class>org.jqassistant.plugin.openapi.api.model.TagDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.openapi.impl.OpenAPIScannerPlugin</class>
    </scanner>
</jqassistant-plugin>